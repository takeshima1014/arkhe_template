  "scripts": {
    "build": "run-s js:min css:min img font webp block",
    "js": "npx webpack --mode development",
    "js:min": "npx webpack --mode production",
    "watch:js": "webpack --watch",
    "watch:gulp": "npx gulp watch",
    "css": "npx gulp sass",
    "css:min": "npx gulp sass --production",
    "img": "npx gulp img",
    "font": "npx gulp font",
    "webp": "npx gulp webp",
    "block": "npx wp-scripts build --config block-builder.js --output-path=blocks --webpack-src-dir=src/blocks",
    "format": "wp-scripts format --config block-builder.js",
		"lint:css": "wp-scripts lint-style --config block-builder.js",
		"lint:js": "wp-scripts lint-js --config block-builder.js",
		"packages-update": "wp-scripts packages-update --config block-builder.js",
		"plugin-zip": "wp-scripts plugin-zip --config block-builder.js",
		"start": "wp-scripts start --config block-builder.js"
  },
  "keywords": [],
  "devDependencies": {
