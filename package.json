    "gulp-replace": "^1.1.3",
    "gulp-sass-glob-use-forward": "^0.1.3",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-webp": "^4.0.1",
    "micromodal": "^0.4.10",
    "npm-run-all": "^4.1.5",
